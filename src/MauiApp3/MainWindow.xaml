<?xml version="1.0" encoding="UTF-8" ?>
<Window
    x:Class="MauiApp3.MainWindow"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp3"
    Title="MauiApp3">
    <Window.Page>
        <local:AppShell />
    </Window.Page>
    <Window.TitleBar>
        <TitleBar
            Title="MauiApp3"
            BackgroundColor="{StaticResource Primary}"
            ForegroundColor="{StaticResource White}"
            HeightRequest="48"
            Icon="dotnet_bot.png"
            Subtitle="Preview">
        </TitleBar>
    </Window.TitleBar>
</Window>
